{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/index.mts",
      "runtimeArgs": [
        "--no-warnings",
        "--experimental-strip-types",
        "--env-file=.env"
      ]
    }
  ]
}
